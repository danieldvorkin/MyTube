<h1> Welcome to MyTube </h1>
<%= link_to "Add New Vid", "videos/new" %>

<% @videos.each do |video| %>
	<p><%= video.title %> :: Published by: <%= video.publisher %></p>
	<iframe width="560" height="315" src="https://www.youtube.com/embed/<%= video.youtube_id %>" frameborder="0" allowfullscreen></iframe><br>
	<%= link_to "Edit Video Info", edit_video_path(video) %> | 
	<%= link_to "Delete", (video), method: :delete, data: {confirm: "Are you sure you want to delete this picture?"}  %>
<% end %>